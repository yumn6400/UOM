virtual int operator!=(const IUnitOfMeasurement &other)=0;
virtual int operator<(const IUnitOfMeasurement &other)=0;
virtual int operator>(const IUnitOfMeasurement &other)=0;
virtual int operator<=(const IUnitOfMeasurement &other)=0;
virtual int operator>=(const IUnitOfMeasurement &other)=0;
};
}
